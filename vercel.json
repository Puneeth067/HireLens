{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [
          "packages/**",
          "server/**"
        ]
      }
    }
  ],
  "installCommand": "cd client && npm install --legacy-peer-deps",
  "buildCommand": "cd client && npm run vercel-build"
}
