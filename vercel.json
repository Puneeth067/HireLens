{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "includeFiles": [
          "client/next.config.js",
          "client/next.config.ts"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "buildCommand": "node build-workspaces.js",
  "installCommand": "npm install",
  "outputDirectory": "client/.next"
}